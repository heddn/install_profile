{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}
{% if items.next %}
  <ul{{ attributes }}>
    <li class="pager__item my-3 py-3 text-center">
      <a class="button border border-gray-600 px-5 rounded-3xl text-blue-500 p-3 hover:bg-gray-300 hover:text-red-500" href="{{ items.next.href }}" rel="next">{{ options.button_text }}</a>
    </li>
  </ul>
{% endif %}
